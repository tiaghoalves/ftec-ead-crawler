{
  "name": "ftec-ead-crawler",
  "version": "1.0.0",
  "description": "Aplicação web crawler para fazer scrapping de dados úteis dos cursos no moodle da Ftec",
  "scripts": {
    "babel-node": "node --require 'dotenv/config' --require 'babel-register'",
    "start": "nodemon --exec npm run babel-node -- ./app.js",
    "build": "babel src -d dist",
    "mocha": "mocha --require babel-core/register",
    "test": "mocha --require babel-core/register --timeout 5000 --recursive"
  },
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "npm": "^6.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "dotenv": "^7.0.0",
    "eslint": "^6.1.0"
  },
  "author": "Tiago Alves (tiago.alves718@gmail.com\t)",
  "license": "ISC"
}
